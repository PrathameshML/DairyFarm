{
  "name": "mali-dairy-farm",
  "version": "1.0.0",
  "description": "Mali Dairy Farm - E-commerce and Training Management System",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client",
    "seed": "node server/scripts/seed.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "razorpay": "^2.9.2",
    "nodemailer": "^6.9.4",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "openai": "^4.0.0",
    "@google/generative-ai": "^0.21.0",
    "pdf-lib": "^1.17.1",
    "exceljs": "^4.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["dairy", "farm", "e-commerce", "training", "agriculture"],
  "author": "Mali Dairy Farm",
  "license": "MIT"
}
